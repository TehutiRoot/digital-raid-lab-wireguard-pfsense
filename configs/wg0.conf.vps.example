# Example WireGuard config for VPS (public side)
# All IPs and keys are MOCK values for documentation only.

[Interface]
Address    = 10.100.0.1/32
ListenPort = 51820
PrivateKey = <REDACTED_VPS_PRIVATE_KEY>

# Optional: if you route traffic out via this VPS
# PostUp   = sysctl -w net.ipv4.ip_forward=1
# PostDown = sysctl -w net.ipv4.ip_forward=0

[Peer]
# pfSense / home gateway
PublicKey  = <PFSENSE_PUBLIC_KEY_PLACEHOLDER>
AllowedIPs = 10.100.0.2/32, 10.10.10.0/24
